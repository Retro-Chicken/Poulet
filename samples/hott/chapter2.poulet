open chapter1

// Lemma 2.1.1
eq_sym : {A : Type1} {x : A} {y : A} Eq[A](x, y) -> Eq[A](y, x)
    := \A : Type1 -> \x : A -> \y : A -> \p : Eq[A](x, y) ->
    path_ind(
        A,
        \x1 : A -> \y1 : A -> \p1 : Eq[A](x1, y1) -> Eq[A](y1, x1),
        \x1 : A -> Eq[A].refl(x1),
        x, y, p
    )

